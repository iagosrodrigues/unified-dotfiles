{
  "coc-actions.showActionKind": true,
  "coc.preferences.snippets.enable": false,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnInsertLeave": false,
  "coc.preferences.formatOnSaveFiletypes": ["typescript"],
  "coc.preferences.formatOnType": false,
  "coc.preferences.highlightTimeout": 200,
  "coc.source.around.firstMatch": true,
  "coc.source.buffer.firstMatch": true,
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.source.file.ignoreHidden": false,
  "codeLens.enable": false,
  "codeLens.separator": " \u00ac ",
  "codeLens.subseparator": " \u2022 ",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "×",
  "diagnostic.hintSign": "ஐ",
  "diagnostic.infoSign": "ღ",
  "diagnostic.messageDelay": 0,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextLines": 3,
  "diagnostic.warningSign": "»",
  "eslint.autoFixOnSave": true,
  "eslint.quiet": true,
  "eslint.run": "onSave",
  "git.addGBlameToBufferVar": false,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.branchCharacter": "\uf418",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "html.autoClosingTags": true,
  "languageserver": {},
  "rust-analyzer.serverPath": "rust-analyzer",
  "rust-analyzer.updates.channel": "nightly",
  "signature.triggerSignatureWait": 200,
  "snippets.autoTrigger": false,
  "snippets.triggerCharacters": [],
  "suggest.enablePreview": true,
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": true,
  "suggest.triggerCompletionWait": 100,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "tsserver.debugPort": 8888,
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.useWorkspaceDependencies": true,
  "vimlsp.indexes.count": 3,
  "vimlsp.indexes.gap": 10,
  "vimlsp.suggest.fromVimruntime": true,
  "rust-analyzer.completion.postfix.enable": false
}
